				{
					"name": "Login",
					"item": [
						{
							"name": "login as user",
							"event": [
								{
									"listen": "test",
									"script": {
										"id": "304444e0-a334-43f8-98e5-9b183c494de3",
										"exec": [
											"const response = JSON.parse(responseBody)",
											"if(response.data.token) {",
											"    if(response.data.role === 'admin') pm.environment.set('admin_token', response.data.token)",
											"    else pm.environment.set('user_token', response.data.token)",
											"}",
											" ",
											"pm.test(\"Status code is 200\", function () {",
											"    pm.response.to.have.status(200);",
											"});",
											"",
											"// Test if status is 'success'",
											"pm.test(\"Status is success\", function () {",
											"    pm.response.to.have.jsonBody(\"status\", \"success\");",
											"});",
											"",
											"// Test if message is 'Login successfully'",
											"pm.test(\"Message is Login successfully\", function () {",
											"    pm.response.to.have.jsonBody(\"message\", \"Login successfully\");",
											"});",
											"",
											"// Test if 'data' is an object",
											"pm.test(\"Data is an object\", function () {",
											"    pm.response.to.have.jsonBody(\"data\").that.is.an('object');",
											"});"
										],
										"type": "text/javascript"
									}
								}
							],
							"request": {
								"method": "POST",
								"header": [
									{
										"key": "Content-Type",
										"name": "Content-Type",
										"value": "application/json",
										"type": "text"
									}
								],
								"body": {
